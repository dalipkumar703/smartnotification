{
    "version": "1.0.2",
    "description": "Smart Notification",
    "manifest_version": 3,
    "background": {
        "service_worker": "service-worker.js"
    },
    "permissions": [
        "tabs",
        "scripting"
      ],
    "name": "Smart Notification",
    "author": "Dalip",
    "action": {
        "default_popup": "index.html",
        "default_title": "Click Me"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "css": ["content.css"],
            "run_at": "document_end"
        }
    ],
    "host_permissions": [
        "https://www.linkedin.com/"
      ]
}
